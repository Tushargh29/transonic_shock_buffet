#!/bin/bash -l
#SBATCH --partition=standard
#SBATCH --nodes=1
#SBATCH --time=48:00:00
#SBATCH --job-name=na12
#SBATCH --ntasks-per-node=8

module load singularity/3.6.0rc2
module load mpi/openmpi/4.0.1/cuda_aware_gcc_6.3.0

mkdir -p run
cp -r test_cases/OnSet_Set1_Ma-0.75_Re-10Mio_GaussLinear_alpha-6_FineMesh run/
cd run/OnSet_Set1_Ma-0.75_Re-10Mio_GaussLinear_alpha-6_FineMesh
./Allrun 8
